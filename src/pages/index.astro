---
import Layout from "../layouts/Layout.astro";
import Wrapper from "../layouts/Wrapper.astro";

import Hero from "../components/Hero.astro";
import Services from "../components/Services.astro";
import WhatToExpect from "../components/WhatToExpect.astro";
import Slider from "../components/Slider.astro";
import Experience from "../components/Experience.astro";
import Corevalues from "../components/Corevalues.astro";
import Contactus from "../components/Contactus.astro";
import Newsletter from "../components/Newsletter.astro";
---

<Layout>
  <Wrapper theme="section_theme_black">
    <Hero />
  </Wrapper>

  <Wrapper theme="section_theme_white">
    <Services />
  </Wrapper>

  <Wrapper theme="section_theme_white">
    <WhatToExpect />
  </Wrapper>

  <Wrapper theme="section_theme_grey">
    <Slider />
  </Wrapper>

  <Wrapper theme="section_theme_yellow">
    <Experience />
  </Wrapper>

  <Wrapper theme="section_theme_black">
    <Corevalues buttonvariant="hero_button" />
  </Wrapper>

  <Wrapper theme="section_theme_white">
    <Contactus />
  </Wrapper>

  <Wrapper theme="section_theme_white">
    <Newsletter />
  </Wrapper>
</Layout>

